{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "request": "launch",
      "name": "Launch Example",
      "type": "node",
      "program": "${workspaceFolder}/example/main.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "/usr/bin/deno",
      "runtimeArgs": [
        "run",
        "--inspect",
        "--allow-all"
      ],
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "ADMIN_PASSWORD": "your-secret-password"
      },
      "attachSimplePort": 9229
    },
    {
      "request": "launch",
      "name": "Custom Handlers Example",
      "type": "node",
      "program": "${workspaceFolder}/example/custom_handlers_example.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "/usr/bin/deno",
      "runtimeArgs": [
        "run",
        "--inspect",
        "--allow-all"
      ],
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "ADMIN_PASSWORD": "your-secret-password"
      },
      "attachSimplePort": 9229
    }
}
